	<parameter name="PRODUTO_DESCRICAO" class="java.lang.String"/>
	<parameter name="FABRICANTE_DESCRICAO" class="java.lang.String"/>
	<parameter name="PRODUTO_REFERENCIA" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT systema.produto.aplicacao,
	systema.produto.descricao AS pdescricao,
	systema.produto.preco,
	systema.produto.quantidade,
	systema.produto.referencia,
	systema.fabricante.descricao AS fdescricao,
	systema.produto.codigo
FROM systema.produto
	INNER JOIN systema.fabricante ON 
	 systema.produto.fabricante_codigo = systema.fabricante.codigo 
WHERE 
	 systema.produto.descricao LIKE $P{PRODUTO_DESCRICAO} 
	 AND systema.fabricante.descricao LIKE $P{FABRICANTE_DESCRICAO} 
	 AND systema.produto.referencia LIKE $P{PRODUTO_REFERENCIA} 
ORDER BY systema.produto.descricao ASC,
	systema.fabricante.descricao ASC]]>
	</queryString>
